<view class="weui-grids">
  <block wx:for="{{menuLists}}" wx:key="unique">
    <navigator url="{{item.pageTo}}" class="weui-grid" hover-class="weui-grid_active">
      <image class="weui-grid__icon" src="{{item.img}}" />
      <view class="weui-grid__label">{{item.name}}</view>
    </navigator>
  </block>
</view>